{
  "name": "medium-post-nestjs-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "docker-compose:down": "docker-compose -f ./docker/docker-compose.yml down",
    "docker-compose:up": "docker-compose -f ./docker/docker-compose.yml up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrations": "typeorm-ts-node-esm migration:run --transaction each -d ./src/db/database.migrations.config.ts",
    "migrations:add": "typeorm migration:create \"./src/db/migrations/migration\"",
    "start": " NODE_ENV=production ENV=production nest start -e 'ts-node -r ./src/env/env.module.ts'",
    "prestart:dev": "npm run docker-compose:down && npm run docker-compose:up &",
    "start:dev": "NODE_ENV=development ENV=development nest start --watch -e 'ts-node -r ./src/env/env.module.ts -r ./src/db/test-data-run.ts'",
    "poststart:dev": "docker-compose:down",
    "start:prod": "NODE_ENV=production ENV=production node -r ./dist/env/env.module dist/main",
    "test": "ENV=test NODE_ENV=test jest",
    "test:cov": "ENV=test NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run docker-compose:down && npm run docker-compose:up &",
    "test:e2e": "ENV=test NODE_ENV=test jest --config ./src/test/jest.config.js",
    "posttest:e2e": "npm run docker-compose:down",
    "test:watch": "ENV=test NODE_ENV=test jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "amazon-cognito-identity-js": "^6.3.1",
    "aws-jwt-verify": "^4.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fetch-mock-jest": "^1.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
